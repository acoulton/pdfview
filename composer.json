{
    "name":        "acoulton/pdfview",
    "description": "View extension for Kohana that renders a PDF using DOMPDF",
    "keywords":    ["kohana"],
    "homepage":    "https://github.com/acoulton/pdfview",
    "type":        "kohana-module",
    "license":     "BSD",
    "authors":     [
        {
            "name":     "Andrew Coulton",
            "email":    "andrew@ingenerator.com",
            "homepage": "http://www.ingenerator.com"
        },
        {
            "name":     "Woody Gilk",
            "homepage": "https://github.com/shadowhand"
        }
    ],
    "require": {
        "php":           ">=5.3.3",
        "kohana/core":   "3.3.*",
        "dompdf/dompdf": "^0.7"
    },
    "require-dev": {
        "kohana/koharness": "*@dev",
        "kohana/unittest":  "3.3.*@dev"
    },
    "support": {
        "issues": "https://github.com/acoulton/pdfview/issues",
        "source": "https://github.com/acoulton/pdfview"
    },
    "bin": [],
    "extra": {
        "branch-alias": {
            "dev-master": "3.3.x-dev"
        },
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:kohana-module"]
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
